# Development Environment Variables

# Application Configuration
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=$npm_package_version
REACT_APP_NAME=ASETT CMS (Dev)
REACT_APP_DESCRIPTION="Administrative Simplification Enforcement and Testing Tool - Development"

# API Configuration
REACT_APP_API_URL=http://localhost:3000
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=30000

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_TRACKING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# Security Configuration
REACT_APP_ENABLE_HTTPS=false
REACT_APP_ENABLE_CORS=true
REACT_APP_CORS_ORIGIN=http://localhost:3000

# Cache Configuration
REACT_APP_CACHE_DURATION=0
REACT_APP_ENABLE_SERVICE_WORKER=false

# Performance Configuration
REACT_APP_ENABLE_COMPRESSION=false
REACT_APP_ENABLE_LAZY_LOADING=true
REACT_APP_ENABLE_CODE_SPLITTING=true

# Monitoring Configuration
REACT_APP_SENTRY_DSN=
REACT_APP_GA_TRACKING_ID=

# CMS Design System Configuration
REACT_APP_CMS_DS_VERSION=7.0.0
REACT_APP_CMS_DS_THEME=core

# Build Configuration
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=true

# Deployment Configuration
REACT_APP_DEPLOYMENT_URL=http://localhost:3000
REACT_APP_DEPLOYMENT_ENV=development

# Error Handling
REACT_APP_ERROR_BOUNDARY_ENABLED=true
REACT_APP_ERROR_REPORTING_ENABLED=true

# Logging Configuration
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_DEBUG_LOGGING=true

# Feature Toggles
REACT_APP_ENABLE_NEW_UI=true
REACT_APP_ENABLE_BETA_FEATURES=true
REACT_APP_ENABLE_MAINTENANCE_MODE=false

# Cache Control
REACT_APP_CACHE_CONTROL=no-cache

# Security Headers
REACT_APP_SECURITY_HEADERS_ENABLED=false
REACT_APP_CSP_ENABLED=false
REACT_APP_HSTS_ENABLED=false

# Performance Monitoring
REACT_APP_ENABLE_WEB_VITALS=true
REACT_APP_ENABLE_PERFORMANCE_METRICS=true

# Accessibility
REACT_APP_ENABLE_ACCESSIBILITY_FEATURES=true
REACT_APP_ENABLE_SCREEN_READER_SUPPORT=true

# Internationalization
REACT_APP_DEFAULT_LOCALE=en-US
REACT_APP_SUPPORTED_LOCALES=en-US

# API Rate Limiting
REACT_APP_API_RATE_LIMIT=1000
REACT_APP_API_RATE_WINDOW=60000

# Session Management
REACT_APP_SESSION_TIMEOUT=7200
REACT_APP_ENABLE_AUTO_LOGOUT=false

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=pdf,doc,docx,xls,xlsx

# Notification Settings
REACT_APP_ENABLE_EMAIL_NOTIFICATIONS=false
REACT_APP_ENABLE_SMS_NOTIFICATIONS=false

# Backup Configuration
REACT_APP_ENABLE_AUTO_BACKUP=false
REACT_APP_BACKUP_INTERVAL=0

# Maintenance Mode
REACT_APP_MAINTENANCE_MESSAGE=""
REACT_APP_MAINTENANCE_START_TIME=
REACT_APP_MAINTENANCE_END_TIME=

# Health Check
REACT_APP_HEALTH_CHECK_INTERVAL=5000
REACT_APP_HEALTH_CHECK_ENDPOINT=/health

# CDN Configuration
REACT_APP_CDN_URL=http://localhost:3000
REACT_APP_CDN_VERSION=$npm_package_version

# API Gateway Configuration
REACT_APP_API_GATEWAY_URL=http://localhost:3000
REACT_APP_API_GATEWAY_TIMEOUT=30000

# Authentication Configuration
REACT_APP_AUTH_ENDPOINT=http://localhost:3000/auth
REACT_APP_AUTH_TIMEOUT=30000
REACT_APP_AUTH_REFRESH_INTERVAL=300000

# Database Configuration
REACT_APP_DB_CONNECTION_TIMEOUT=5000
REACT_APP_DB_RETRY_ATTEMPTS=1

# Queue Configuration
REACT_APP_QUEUE_TIMEOUT=5000
REACT_APP_QUEUE_RETRY_ATTEMPTS=1

# Cache Invalidation
REACT_APP_CACHE_INVALIDATION_INTERVAL=0
REACT_APP_CACHE_INVALIDATION_ENABLED=false

# Error Reporting
REACT_APP_ERROR_REPORTING_ENDPOINT=http://localhost:3000/error-reporting
REACT_APP_ERROR_REPORTING_SAMPLE_RATE=1.0

# Performance Budget
REACT_APP_PERFORMANCE_BUDGET_FIRST_CONTENTFUL_PAINT=3000
REACT_APP_PERFORMANCE_BUDGET_TIME_TO_INTERACTIVE=5000
REACT_APP_PERFORMANCE_BUDGET_SPEED_INDEX=5.0

# Security Scanning
REACT_APP_SECURITY_SCAN_INTERVAL=0
REACT_APP_SECURITY_SCAN_ENABLED=false

# Compliance
REACT_APP_HIPAA_COMPLIANCE_ENABLED=false
REACT_APP_ADA_COMPLIANCE_ENABLED=true
REACT_APP_508_COMPLIANCE_ENABLED=true

# Monitoring Thresholds
REACT_APP_ERROR_RATE_THRESHOLD=0.1
REACT_APP_RESPONSE_TIME_THRESHOLD=2000
REACT_APP_AVAILABILITY_THRESHOLD=0.95

# Feature Flags for Gradual Rollout
REACT_APP_ENABLE_NEW_NAVIGATION=true
REACT_APP_ENABLE_NEW_DASHBOARD=true
REACT_APP_ENABLE_NEW_REPORTING=true
REACT_APP_ENABLE_NEW_ANALYTICS=true

# API Versioning
REACT_APP_API_VERSION_HEADER=X-API-Version
REACT_APP_API_VERSION=v1
REACT_APP_API_DEPRECATION_WARNING=false

# Rate Limiting Configuration
REACT_APP_RATE_LIMIT_WINDOW=60000
REACT_APP_RATE_LIMIT_MAX_REQUESTS=1000
REACT_APP_RATE_LIMIT_STRATEGY=sliding

# Cache Strategy
REACT_APP_CACHE_STRATEGY=network-first
REACT_APP_CACHE_MAX_AGE=0
REACT_APP_CACHE_STALE_WHILE_REVALIDATE=0

# Service Worker Configuration
REACT_APP_SERVICE_WORKER_UPDATE_INTERVAL=0
REACT_APP_SERVICE_WORKER_CACHE_NAME=asett-cms-dev
REACT_APP_SERVICE_WORKER_SKIP_WAITING=true

# Build Optimization
REACT_APP_ENABLE_TREE_SHAKING=true
REACT_APP_ENABLE_CODE_SPLITTING=true
REACT_APP_ENABLE_LAZY_LOADING=true
REACT_APP_ENABLE_PREFETCHING=false

# Development Tools
REACT_APP_ENABLE_DEV_TOOLS=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_DEBUG_LOGGING=true 