# Production Environment Variables

# Application Configuration
REACT_APP_ENVIRONMENT=production
REACT_APP_VERSION=$npm_package_version
REACT_APP_NAME=ASETT CMS
REACT_APP_DESCRIPTION="Administrative Simplification Enforcement and Testing Tool"

# API Configuration
REACT_APP_API_URL=https://api.asett.cms.gov
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=30000

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_TRACKING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# Security Configuration
REACT_APP_ENABLE_HTTPS=true
REACT_APP_ENABLE_CORS=true
REACT_APP_CORS_ORIGIN=https://asett.cms.gov

# Cache Configuration
REACT_APP_CACHE_DURATION=3600
REACT_APP_ENABLE_SERVICE_WORKER=true

# Performance Configuration
REACT_APP_ENABLE_COMPRESSION=true
REACT_APP_ENABLE_LAZY_LOADING=true
REACT_APP_ENABLE_CODE_SPLITTING=true

# Monitoring Configuration
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@asett.cms.gov/1
REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X

# CMS Design System Configuration
REACT_APP_CMS_DS_VERSION=7.0.0
REACT_APP_CMS_DS_THEME=core

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false

# Deployment Configuration
REACT_APP_DEPLOYMENT_URL=https://asett.cms.gov
REACT_APP_DEPLOYMENT_ENV=production

# Error Handling
REACT_APP_ERROR_BOUNDARY_ENABLED=true
REACT_APP_ERROR_REPORTING_ENABLED=true

# Logging Configuration
REACT_APP_LOG_LEVEL=error
REACT_APP_ENABLE_DEBUG_LOGGING=false

# Feature Toggles
REACT_APP_ENABLE_NEW_UI=true
REACT_APP_ENABLE_BETA_FEATURES=false
REACT_APP_ENABLE_MAINTENANCE_MODE=false

# Cache Control
REACT_APP_CACHE_CONTROL=max-age=31536000,public

# Security Headers
REACT_APP_SECURITY_HEADERS_ENABLED=true
REACT_APP_CSP_ENABLED=true
REACT_APP_HSTS_ENABLED=true

# Performance Monitoring
REACT_APP_ENABLE_WEB_VITALS=true
REACT_APP_ENABLE_PERFORMANCE_METRICS=true

# Accessibility
REACT_APP_ENABLE_ACCESSIBILITY_FEATURES=true
REACT_APP_ENABLE_SCREEN_READER_SUPPORT=true

# Internationalization
REACT_APP_DEFAULT_LOCALE=en-US
REACT_APP_SUPPORTED_LOCALES=en-US

# API Rate Limiting
REACT_APP_API_RATE_LIMIT=100
REACT_APP_API_RATE_WINDOW=60000

# Session Management
REACT_APP_SESSION_TIMEOUT=3600
REACT_APP_ENABLE_AUTO_LOGOUT=true

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=pdf,doc,docx,xls,xlsx

# Notification Settings
REACT_APP_ENABLE_EMAIL_NOTIFICATIONS=true
REACT_APP_ENABLE_SMS_NOTIFICATIONS=false

# Backup Configuration
REACT_APP_ENABLE_AUTO_BACKUP=true
REACT_APP_BACKUP_INTERVAL=86400

# Maintenance Mode
REACT_APP_MAINTENANCE_MESSAGE="System is currently under maintenance. Please try again later."
REACT_APP_MAINTENANCE_START_TIME=
REACT_APP_MAINTENANCE_END_TIME=

# Health Check
REACT_APP_HEALTH_CHECK_INTERVAL=30000
REACT_APP_HEALTH_CHECK_ENDPOINT=/health

# CDN Configuration
REACT_APP_CDN_URL=https://cdn.asett.cms.gov
REACT_APP_CDN_VERSION=$npm_package_version

# API Gateway Configuration
REACT_APP_API_GATEWAY_URL=https://api-gateway.asett.cms.gov
REACT_APP_API_GATEWAY_TIMEOUT=30000

# Authentication Configuration
REACT_APP_AUTH_ENDPOINT=https://auth.asett.cms.gov
REACT_APP_AUTH_TIMEOUT=30000
REACT_APP_AUTH_REFRESH_INTERVAL=300000

# Database Configuration
REACT_APP_DB_CONNECTION_TIMEOUT=10000
REACT_APP_DB_RETRY_ATTEMPTS=3

# Queue Configuration
REACT_APP_QUEUE_TIMEOUT=30000
REACT_APP_QUEUE_RETRY_ATTEMPTS=3

# Cache Invalidation
REACT_APP_CACHE_INVALIDATION_INTERVAL=3600000
REACT_APP_CACHE_INVALIDATION_ENABLED=true

# Error Reporting
REACT_APP_ERROR_REPORTING_ENDPOINT=https://error-reporting.asett.cms.gov
REACT_APP_ERROR_REPORTING_SAMPLE_RATE=1.0

# Performance Budget
REACT_APP_PERFORMANCE_BUDGET_FIRST_CONTENTFUL_PAINT=1800
REACT_APP_PERFORMANCE_BUDGET_TIME_TO_INTERACTIVE=3500
REACT_APP_PERFORMANCE_BUDGET_SPEED_INDEX=3.4

# Security Scanning
REACT_APP_SECURITY_SCAN_INTERVAL=86400
REACT_APP_SECURITY_SCAN_ENABLED=true

# Compliance
REACT_APP_HIPAA_COMPLIANCE_ENABLED=true
REACT_APP_ADA_COMPLIANCE_ENABLED=true
REACT_APP_508_COMPLIANCE_ENABLED=true

# Monitoring Thresholds
REACT_APP_ERROR_RATE_THRESHOLD=0.01
REACT_APP_RESPONSE_TIME_THRESHOLD=1000
REACT_APP_AVAILABILITY_THRESHOLD=0.99

# Feature Flags for Gradual Rollout
REACT_APP_ENABLE_NEW_NAVIGATION=false
REACT_APP_ENABLE_NEW_DASHBOARD=false
REACT_APP_ENABLE_NEW_REPORTING=false
REACT_APP_ENABLE_NEW_ANALYTICS=false

# API Versioning
REACT_APP_API_VERSION_HEADER=X-API-Version
REACT_APP_API_VERSION=v1
REACT_APP_API_DEPRECATION_WARNING=true

# Rate Limiting Configuration
REACT_APP_RATE_LIMIT_WINDOW=60000
REACT_APP_RATE_LIMIT_MAX_REQUESTS=100
REACT_APP_RATE_LIMIT_STRATEGY=sliding

# Cache Strategy
REACT_APP_CACHE_STRATEGY=stale-while-revalidate
REACT_APP_CACHE_MAX_AGE=31536000
REACT_APP_CACHE_STALE_WHILE_REVALIDATE=86400

# Service Worker Configuration
REACT_APP_SERVICE_WORKER_UPDATE_INTERVAL=3600000
REACT_APP_SERVICE_WORKER_CACHE_NAME=asett-cms-v1
REACT_APP_SERVICE_WORKER_SKIP_WAITING=true

# Build Optimization
REACT_APP_ENABLE_TREE_SHAKING=true
REACT_APP_ENABLE_CODE_SPLITTING=true
REACT_APP_ENABLE_LAZY_LOADING=true
REACT_APP_ENABLE_PREFETCHING=true

# Development Tools
REACT_APP_ENABLE_DEV_TOOLS=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false
REACT_APP_ENABLE_DEBUG_LOGGING=false 